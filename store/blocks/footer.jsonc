  // Last Footer
  "flex-layout.row#footer-last": {
    "children": [
      "flex-layout.col#webpay",
      "flex-layout.col#partners",
      "flex-layout.col#derechos"
    ],
    "props": {
      "blockClass": "row-lastFooter"
    }
  },

  "flex-layout.col#webpay": {
    "children": ["image#transbank-compra-segura"],
    "props": {
      "blockClass": "lastFooter-webpay"
    }
  },

  "flex-layout.col#partners": {
    "children": ["image#sr", "image#vtex"],
    "props": {
      "blockClass": "lastFooter-partners"
    }
  },

  "flex-layout.col#derechos": {
    "children": ["rich-text#derechos"],
    "props": {
      "blockClass": "lastFooter-derechos"
    }
  },

  "image#transbank-compra-segura": {
    "props": {
      "blockClass": "lastFooter-segura",
      "src": "assets/img/webpay-comrpa-segura.png"
    }
  },

  "image#sr": {
    "props": {
      "blockClass": "lastFooter-sr",
      "src": "assets/img/logo-sr-blanco.png"
    }
  },

  "image#vtex": {
    "props": {
      "blockClass": "lastFooter-vtex",
      "src": "assets/img/logo-vtex-blanco.png"
    }
  },

  "rich-text#derechos": {
    "props": {
      "blockClass": "lastFooter-derecho",
      "text": "Gourmet todos los derechos reservados "
    }
  }
}
