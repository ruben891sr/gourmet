{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": ["flex-layout.row#footer-main", "flex-layout.row#footer-last"]
  },
  "footer-layout.mobile": {
    "children": ["flex-layout.row#footer-main", "flex-layout.row#footer-last"]
  },

  // Footer Main
  "flex-layout.row#footer-main": {
    "children": [
      "flex-layout.col#corporativo",
      "flex-layout.col#sac",
      "flex-layout.col#rrss"
    ],
    "props": {
      "blockClass": "row-footerMain"
    }
  },

  "flex-layout.col#corporativo": {
    "children": [
      "rich-text#footerMain-corporativo",
      "vtex.menu@2.x:menu#corporativo"
    ],
    "props": {
      "blockClass": "col-footerMain"
    }
  },

  "flex-layout.col#sac": {
    "children": ["rich-text#footerMain-sac", "vtex.menu@2.x:menu#sac"],
    "props": {
      "blockClass": "col-footerMain"
    }
  },

  "flex-layout.col#rrss": {
    "children": ["rich-text#footerMain-rrss", "vtex.menu@2.x:menu#rrss"],
    "props": {
      "blockClass": "col-footerMain"
    }
  },

  "rich-text#footerMain-corporativo": {
    "props": {
      "text": "### Corporativo",
      "blockClass": "footerMainTitle"
    }
  },

  "vtex.menu@2.x:menu#corporativo": {
    "children": [
      "menu-item#empresa-gourmet"
      // "menu-item#compras-mayor"
    ],
    "props": {
      "blockClass": "footerMainMenu"
    }
  },

  "rich-text#footerMain-sac": {
    "props": {
      "text": "### Servicio al cliente",
      "blockClass": "footerMainTitle"
    }
  },

  "vtex.menu@2.x:menu#sac": {
    "children": [
      "menu-item#contacto-footer",
      // "menu-item#seguimiento",
      "menu-item#faq",
      "menu-item#tyc",
      "menu-item#politica-privacidad"
    ],
    "props": {
      "blockClass": "footerMainMenu"
    }
  },

  "rich-text#footerMain-rrss": {
    "props": {
      "text": "### Redes Sociales",
      "blockClass": "footerMainTitle"
    }
  },

  "vtex.menu@2.x:menu#rrss": {
    "children": [
      "menu-item#face",
      "menu-item#insta",
      "menu-item#yt",
      "menu-item#pinter"
    ],
    "props": {
      "blockClass": "footerMainMenu"
    }
  },

  "menu-item#empresa-gourmet": {
    "props": {
      "blockClass": "menuFooterMainItem",
      "id": "menu-item-empresa-gourmet",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/empresa",
        "noFollow": false,
        "tagTitle": "Empresa Gourmet",
        "text": "Empresa Gourmet"
      }
    }
  },

