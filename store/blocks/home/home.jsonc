{
  "store.home": {
    "blocks": ["responsive-layout.desktop", "responsive-layout.mobile"]
  },

  // General versiones Mobile Desktop
  "responsive-layout.desktop": {
    "children": [
      "flex-layout.row#infoBar",
      "list-context.image-list#slider",
      "__fold__.experimentalLazyAssets",
      // "flex-layout.row#bn-dcto",
      "flex-layout.row#category-home-title",
      "flex-layout.row#category-home",
      "flex-layout.row#category-home-vertodo",
      "flex-layout.row#shelf-title",
      "list-context.product-list#home",
      // "flex-layout.row#ventas-por",
      // "flex-layout.row#cotiza",
      "flex-layout.row#newsletter"
    ]
  },

  "responsive-layout.mobile": {
    "children": [
      "flex-layout.row#infoBar-mobile",
      "list-context.image-list#slider",
      "__fold__.experimentalLazyAssets",
      // "flex-layout.row#bn-dcto",
      "flex-layout.row#category-home-title",
      "flex-layout.row#category-home",
      "flex-layout.row#shelf-title",
      "list-context.product-list#home",
      // "flex-layout.row#ventas-por",
      // "flex-layout.row#cotiza",
      "flex-layout.row#newsletter"
    ]
  },

  // Info Bar
  "flex-layout.row#infoBar": {
    "children": [
      "rich-text#col-1-infobar",
      "rich-text#col-2-infobar",
      "rich-text#col-3-infobar"
    ],
    "props": {
      "blockClass": "row-InfoBar"
    }
  },
  "rich-text#col-1-infobar": {
    "props": {
      "text": "Estás en el portal de compras al por mayor de Gourmet. Para poder comprar bajo esta modalidad, debes cumplir con los siguientes requisitos:",
      "blockClass": "infoBarCol-text"
    }
  },
  "rich-text#col-2-infobar": {
    "props": {
      "text": "- Tener RUT empresa\n- Monto mínimo de compras de $50.000\n- Cada producto debe ser comprado por caja ",
      "blockClass": "infoBarCol-text"
    }
  },
  "rich-text#col-3-infobar": {
    "props": {
      "text": "Para ver información de despacho y costos, haz click acá",
      "blockClass": "infoBarCol-text"
    }
  },

  // Desktop - Slider Principal
  "list-context.image-list#slider": {
    "children": ["slider-layout#demo-images"],
    "props": {
      "height": 550,
      "blockClass": "sliderImageNew",
      "images": [
        {
          "image": "assets/img/slider-escri-1.jpg",
          "mobileImage": "assets/img/slider-escri-1-mobile.jpg"
        },
        {
          "image": "assets/img/slider-escri-2.jpg",
          "mobileImage": "assets/img/slider-escri-2-mobile.jpg"
        },
        {
          "image": "assets/img/slider-escri-3.jpg",
          "mobileImage": "assets/img/slider-escri-3-mobile.jpg"
        }
      ]
    }
  },

  "slider-layout#demo-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    }
  },

  // Imagen Promo
  "flex-layout.row#bn-dcto": {
    "children": ["image#info-image"],
    "props": {
      "fullWidth": false,
      "blockClass": "row-bn-dcto",
      "marginTop": 8,
      "marginBottom": 8
    }
  },

  "image#info-image": {
    "props": {
      "src": "assets/img/bn-dcto.jpg",
      "link": "",
      "width": "100%",
      "blockClass": "bn-dcto"
    }
  },

  // Categorias Home
  "flex-layout.row#category-home-title": {
    "children": ["rich-text#category-home-title"],
    "props": {
      "blockClass": "row-categoryHomeTitle"
    }
  },
  "rich-text#category-home-title": {
    "props": {
      "text": "## Categorías",
      "blockClass": "shelfTitle"
    }
  },

  "flex-layout.row#category-home": {
    "children": [
      "info-card#food-service",
      "info-card#reposteria",
      "info-card#esencias",
      "info-card#salsas-postres",
      "info-card#salsas-condimientadas",
      "info-card#hiervas-especias",
      "info-card#caldos",
      "info-card#cremas-sopas",
      "info-card#basas-salsas",
      "info-card#alinos-ensaladas",
      "info-card#salsa-untar",
      "info-card#delicatesen"
      // "info-card#mas-matural",
