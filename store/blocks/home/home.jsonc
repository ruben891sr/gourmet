  "flex-layout.row#category-home-vertodo": {
    "children": ["rich-text#category-home-vertodo"],
    "props": {
      "blockClass": "row-category-vertodo"
    }
  },
  "rich-text#category-home-vertodo": {
    "props": {
      "text": "[Ver todos los productos](/138?map=productClusterIds)",
      "blockClass": "category-vertodo"
    }
  },


  //Shelf Home
  "flex-layout.row#shelf-title": {
    "children": ["rich-text#shelf-title"],
    "props": {
      "blockClass": "row-shelfTitle"
    }
  },

  "rich-text#shelf-title": {
    "props": {
      "text": "## Productos Destacados",
      "blockClass": "shelfTitle"
    }
  },

  "list-context.product-list#home": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#demo-products"],
    "props": {
      "orderBy": "OrderByTopSaleDESC"
      // "collection": "140"
    }
  }, 
 
  "slider-layout#demo-products": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1 
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },

  // Ventas por
  "flex-layout.row#ventas-por": {
    "children": ["info-card#lote", "info-card#sku"],
    "props": {
      "blockClass": "row-ventas-por",
      "marginTop": 8
    }
  },

  "info-card#lote": {
    "props": {
      "id": "info-card-lote",
      "isFullModeStyle": true,
      "textPosition": "center",
      "imageUrl": "assets/img/bn-venta-lote.jpg",
      // "headline": "Ventas por lote",
      "callToActionText": "Ventas por lote",
      "callToActionUrl": "/quickorder",
      "blockClass": "info-card-ventas",
      "textAlignment": "center"
    }
  },

  "info-card#sku": {
    "props": {
      "id": "info-card-sku",
      "isFullModeStyle": true,
      "textPosition": "center",
      "imageUrl": "assets/img/bn-venta-sku.jpg",
      // "headline": "Ventas por sku",
      "callToActionText": "Ventas por sku",
      "callToActionUrl": "/quickorder",
      "blockClass": "info-card-ventas",
      "textAlignment": "center"
    }
  },

  // Cotiza
  "flex-layout.row#cotiza": {
    "children": ["info-card#cotiza"],
    "props": {
      "blockClass": "row-cotiza",
      "marginBottom": 8,
      "marginTop": 8
    }
  },

  "info-card#cotiza": {
    "props": {
      "id": "info-card-cotiza",
      "isFullModeStyle": true,
      "textPosition": "center",
      "imageUrl": "assets/img/bn-cotiza.jpg",
      "headline": "Cotiza con nosotros",
      "callToActionText": "Cotiza ahora",
      "callToActionUrl": "/sale/d",
      "blockClass": "info-card-cotiza",
      "textAlignment": "center"
    }
  },

  // Newsletter
  "flex-layout.row#newsletter": {
    "children": [
      "rich-text#newsletter-title",
      "rich-text#newsletter-subtitle",
      "form#newsletter",
      "flex-layout.row#social-media-newsletter"
    ], 
    "props": {
      "blockClass": "row-newsletter",
      "colSizing": "auto"
    }
  },

  "rich-text#newsletter-title": {
    "props": {
      "text": "## Suscríbete a nuestros newsletter",
      "blockClass": "newsletter-title"
    }
  },

  "rich-text#newsletter-subtitle": {
    "props": {
      "text": "y obtén un código con un 10% de descuento en tu primer compra",
      "blockClass": "newsletter-subtitle"
    }
  },

  "form#newsletter": {
    "props": {
      "entity": "newsltter",
      "schema": "newsltter",
      "blockClass": "form-newsletter"
    },
    "children": [
      "flex-layout.row#row-newsletter-presonal",
      "flex-layout.row#row-newsletter-suscribeme",
      "form-submit#submit-newsletter"
    ],
    "blocks": ["form-success#newsletter"]
  },

  "flex-layout.row#row-newsletter-presonal": {
    "children": [
      "form-input.text#nombre-newsletter",
      "form-input.text#apellido-newsletter",
      "form-input.text#mail-newsletter"
    ],
    "props": {
      "blockClass": "row-newsletter-presonal"
    }
  },

  "flex-layout.row#row-newsletter-suscribeme": {
    "children": ["form-input.dropdown#suscribeme-newsletter"],
    "props": {
      "blockClass": "row-newsletter-suscribeme"
    }
  },

  "form-input.text#nombre-newsletter": {
    "props": {
      "pointer": "#/properties/nombre",
      "placeholder": "ingresa tu nombre",
      "blockClass": "newsletter-input"
    }
  },

  "form-input.text#apellido-newsletter": {
    "props": {
      "pointer": "#/properties/apellido",
      "placeholder": "ingresa tu apellido",
      "blockClass": "newsletter-input"
    }
  },

  "form-input.text#mail-newsletter": {
    "props": {
      "pointer": "#/properties/mail",
      "placeholder": "ingresa tu email",
      "blockClass": "newsletter-input"
    }
  },

  "form-input.dropdown#suscribeme-newsletter": {
    "props": {
      "pointer": "#/properties/suscripcion/properties/selectTipo"
    }
  },

  "form-submit#submit-newsletter": {
    "props": {
      "label": "Suscribirme",
      "blockClass": "submit-newsletter"
    }
  },

  "form-success#newsletter": {
    "children": ["rich-text#successSubmit-newsletter"]
  },
  "rich-text#successSubmit-newsletter": {
    "props": {
      "text": "Datos enviados con éxito!!",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },

  "flex-layout.row#social-media-newsletter": {
    "children": [
      "rich-text#text-social-media-newsletter",
      "flex-layout.row#social-media-images"
    ],
    "props": {
      "blockClass": "row-socialMedia-newsletter",
      "colSizing": "auto"
    }
  },

  "rich-text#text-social-media-newsletter": {
    "props": {
      "blockClass": "text-social-media-newsletter",
      "text": "También estamos en redes sociales. Síguenos y entérate de todas las recetas y novedades"
    }
  },

  "flex-layout.row#social-media-images": {
    "children": [
      "image#insta-newsletter",
      "image#face-newsletter",
      "image#yt-newsletter",
      "image#pinter-newsletter"
    ],
    "props": {
      "blockClass": "row-socialmedia-images"
    }
  },

  "image#insta-newsletter": {
    "props": {
      "src": "assets/img/news-insta.png",
      "width": 20,
      "blockClass": "newsletter-insta",
      "link": "https://www.instagram.com/gourmetchile/"
    }
  },

  "image#face-newsletter": {
    "props": {
      "src": "assets/img/news-face.png",
      "width": 20,
      "blockClass": "newsletter-face",
      "link": "https://www.facebook.com/GourmetChile/"
    }
  },

  "image#yt-newsletter": {
    "props": {
      "src": "assets/img/news-yt.png",
      "width": 20,
      "blockClass": "newsletter-yt",
      "link": "https://www.youtube.com/channel/UCjJzRDi9jrfyA7GJ1UX4z_Q"
    }
  },

  "image#pinter-newsletter": {
    "props": {
      "src": "assets/img/news-pinter.png",
      "width": 20,
      "blockClass": "newsletter-pinter",
      "link": "https://www.pinterest.cl/Gourmet_Chile/_created/"
    }
  },

  "flex-layout.row#shelf": {
    "children": ["shelf"]
    // "children": ["list-context.product-list#demo1"]
  },

  "rich-text#question": {
    "props": {
      "text": "**Esta es una muestra de como sería tu sitio.\nQuieres saber más?**",
      "blockClass": "question"
    }
  },

  "rich-text#link": {
    "props": {
      "text": "\n**Contáctanos en**\nwww.sr-consultores.com",
      "blockClass": "link"
    }
  },

  // Info Bar
  "flex-layout.row#infoBar-mobile": {
    "children": [
      "rich-text#col-1-infobar-mobile",
      "rich-text#col-2-infobar-mobile"
    ],
    "props": {
      "blockClass": "row-InfoBar"
    }
  },
  "rich-text#col-1-infobar-mobile": {
    "props": {
      "text": "Para comprar en Gourmet debes cumplir con los siguiente:",
      "blockClass": "infoBarCol-text"
    }
  },
  "rich-text#col-2-infobar-mobile": {
    "props": {
      "text": "- Tener RUT empresa\n- Monto mínimo de compras de $50.000\n- Cada producto debe ser comprado por caja ",
      "blockClass": "infoBarCol-text"
    }
  }
}
